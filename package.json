{
  "name": "piqure",
  "version": "2.1.2",
  "description": "A library to provide and inject for JavaScript and TypeScript",
  "main": "dist/Piqure.js",
  "typings": "src/Piqure.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ci": "tsc --noEmit && vitest run --coverage",
    "test:coverage": "vitest --coverage",
    "lint:ci": "eslint",
    "lint": "eslint --fix",
    "format:ci": "prettier --check .",
    "format": "prettier --write ."
  },
  "repository": "github:Gnuk/piqure",
  "bugs": {
    "url": "https://github.com/Gnuk/piqure/issues"
  },
  "homepage": "https://github.com/Gnuk/piqure#readme",
  "author": "Anthony REY",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^9.35.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.26.0",
    "jiti": "^2.5.1",
    "prettier": "^3.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^4.0.0"
  }
}
